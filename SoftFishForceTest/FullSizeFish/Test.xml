                                    <geom name="tail5left" type="box" pos="30 1.25 75" size="2 1.25 2" rgba=".5 .7 .9 1" mass="0.01" fluidshape="ellipsoid" contype="0" conaffinity="0"/>
                                    <geom name="tail5right" type="box" pos="30 -1.25 75" size="2 1.25 2" rgba=".5 .7 .9 1" mass="0.01" fluidshape="ellipsoid" contype="0" conaffinity="0"/>
                                    <site name="tail5left" pos="30 -2.15 75" size="3.75" rgba=".9 .6 .5 .5"/>
                                    <site name="tail5right" pos="30 2.15 75" size="3.75" rgba=".9 .6 .5 .5"/>

                                    <!--<body pos="-140 0 0" name="fishTail">-->
                                    <geom name="finTail" type="mesh"  mesh="tailMesh" pos="-140 0 0" quat="0.7071 0.7071 0 0" rgba=".9 .9 .9 1" mass="0.001" fluidshape="ellipsoid" contype="0" conaffinity="0"/>
                                            <!--<geom name="finTail" type="mesh" mesh="finTail" pos="0.015999999999999997 0 0" rgba=".9 .2 .2 0.7" mass="0.01" fluidshape="ellipsoid"/>-->
                                            <!-- Biggest change from TendonFish is removing these lines:                                             <geom name="tailLeft" type="box" pos="170 0 75" size="2 1.25 2" rgba=".9 .9 .9 1" mass="0.1" contype="1" conaffinity="1" />
                                            <geom name="tailRight" type="box" pos="170 0 75" size="2 1.25 2" rgba=".9 .9 .9 1" mass="0.1" contype="1" conaffinity="1" />
                                            <site name="tailLeft" pos="170 -2 75" size="0.2" rgba="0 255 0 1" />
                                            <site name="tailRight" pos="170 2 75" size="0.2" rgba="0 255 0 1" /> -->
                                    <site name="marker10" pos="30 0 75" size="3.75" material="marker"/>
                                </body>
                            </body>
                        </body>
                    </body>
                </body>
            </body>
        </body>
    </worldbody>

    <tendon>
         <spatial name="tendonLeft" range="237.24 237.3 "> <!-- stiffness="1e5" damping="0"--> 
            <site site="bodyLeft"/>
            <site site="tail0left"/> 
            <site site="tail1left"/> 
            <site site="tail2left"/>
            <site site="tail3left"/> 
            <site site="tail4left"/>
            <site site="tail5left"/> 
            <!--<site site="tailLeft"/>-->
            
        </spatial>

         <spatial name="tendonRight" range="237.24 237.3 "> 
            <site site="bodyRight"/>
            <site site="tail0right"/>
            <site site="tail1right"/> 
            <site site="tail2right"/>
            <site site="tail3right"/> 
            <site site="tail4right"/> 
            <site site="tail5right"/> 
            <!--<site site="tailRight"/>-->
        </spatial>        
    </tendon>


    <actuator>
        <!-- Position actuator for direct position control. Need to work on increasing the speed of the simulation-->
        <!--<position name="rotating_motor" joint="motor_rotate" kp="1e7"  kv="1e6" ctrllimited="true" ctrlrange="-1 1" gear=".15"/>-->
        <general name="left_act" tendon="tendonLeft" ctrllimited="true" gear="5" ctrlrange="-50 50"/>
        <general name="right_act" tendon="tendonRight" ctrllimited="true" gear="5" ctrlrange="-50 50"/>
        <!--<velocity name="rotating_motor" joint="motor_rotate" kv="100" ctrllimited="true" ctrlrange="-1000 1000" forcerange="-250 250" gear="10"/>-->
    </actuator>

