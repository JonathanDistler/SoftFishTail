<!--Creates a tendon fish based on Mike Yan Michelis' design.-->
<!-- Builds a life-sized fish-->
<!--As of now, the forces aren't super accurate to real life -as well as the masses- so that will be the next step, along with making the simulation progress faster-->

<mujoco model="tendonFish">
<option gravity="0 0 0" density="1000" viscosity="0.0013" integrator="implicitfast" timestep="0.001" iterations="100" solver="Newton" tolerance="1e-15" ls_tolerance="1e-9" noslip_tolerance="1e-9" o_solimp="0 0.01 0.01 0.1 2"> 
        <!--Prev timestep was .001-->
        <flag contact="enable"/>
        <!-- <flag override="enable"/> also, messes -->
    </option>

    <visual>
        <global offheight="2160" offwidth="3840"/>
        <rgba haze=".3 .3 .3 1"/>
    </visual>

    <asset>
        <texture type="skybox" builtin="gradient" rgb1="0.6 0.6 0.6" rgb2="0 0 0" width="512" height="512"/>
        <texture name="texplane" type="2d" builtin="checker" rgb1=".15 .2 .35" rgb2=".2 .3 .45" width="512" height="512" mark="cross" markrgb=".8 .8 .8"/>
        <material name="matplane" reflectance="0.3" texture="texplane" texrepeat="10 10" texuniform="true"/>
        <mesh name="tailMesh" file="C:\Users\15405\OneDrive\Desktop\Career\ETHZ\ETHZ Work\mujoco-3.3.2-windows-x86_64\model\mesh\tailV2.STL" scale="1 1 1" />
        <mesh name="headMesh" file="C:\Users\15405\OneDrive\Desktop\Career\ETHZ\ETHZ Work\mujoco-3.3.2-windows-x86_64\model\mesh\headExternal.STL" scale="1 -1 1" />
        <mesh name="planetaryGearBox" file="C:\Users\15405\OneDrive\Desktop\Career\ETHZ\ETHZ Work\mujoco-3.3.2-windows-x86_64\model\mesh\sunGear.STL" scale="1 1 1" />


        <material name="acrylic" rgba=".9 .9 .9 0.2"/>
        <material name="alu" reflectance="0.9" specular="0.9" shininess="0.9" rgba=".7 .7 .7 1"/>
        <material name="marker" rgba=".9 .4 .1 .5"/>
        <!--density="1240" maybe change back the objects with PLA to alu or acrylic-->
        <!-- <material name="PLA" rgba="0 255 248 .8 " />-->
    </asset>

    <!--Play with different tendon widths-->
    <default>
        <joint type="hinge" pos="0 0 0" axis="0 0 1" range="-80 80" stiffness="1" damping="0.1"/>
        <!--originally stifness was .65 damping was 0-->
        <geom fluidcoef="0.1 0.1 0.1 2.0 1.0" condim="1"/>
        <tendon width=".75" rgba=".9 .6 .5 1" stiffness="5" damping="1"/>
        <!-- tendon prev stiffness=20 and damp=10-->
    </default>

    <worldbody>
        <camera name="fixed" pos="0.4 -1.2 1.2" xyaxes="1 0 0 0 1 1"/>
        <camera name="fixedTop" pos="-0.75 0 1.5" xyaxes="0 -1 0 1 0 0.75"/>
        <camera name="fixedDiag" pos="0.75 -1 1" xyaxes="0.8 0.6 0 -0.4 0.5 0.75"/>
        <camera pos="0.625 -0.592 0.720" xyaxes="0.758 0.653 -0.000 -0.468 0.544 0.697"/>



        <geom name="floor" pos="0 0 -0.2" size="0 0 1" type="plane" material="matplane"/>
        <geom name="ceiling" pos="0 0 0.2" size="0 0 1" zaxis="0 0 -1" type="plane" rgba=".7 .1 .1 0"/>
        <light directional="true" diffuse=".8 .8 .8" specular=".2 .2 .2" pos="0 0 5" dir="0 0 -1"/>

        <body pos="0 0 0">
            <joint name="headX" pos="0 0 0" type="slide" axis="1 0 0" stiffness="0" damping="0"/>
            <site name="COM" pos="0 0 0" size="2" material="marker"/>
            <geom name="head_mesh" type="mesh" mesh="headMesh" pos="0 -37.5 145" material="acrylic" density="11.8" fluidshape="ellipsoid" quat="0 0 0.7071 0.7071" contype="1" conaffinity="1" />
            
            <site name="marker0" pos="0 3.5 75" size="3.75" material="marker"/>
            
            <body pos="20 3.25 75" name="motor">
                <!-- Slide joint along x-axis with limits -->
                <joint name="motor_rotate" type="hinge" axis="0 0 1" pos="0 0 0" limited="false" stiffness="0" damping="0"/>
                <!-- Rotating body similar to the gearbox in the fish -->
                <geom name="motorSlider" type="cylinder" pos="0 0 0" size="3.5 5" rgba="0.59 0.8 1 .8" mass=".05" contype="1" conaffinity="1" />
                <geom name="motorRods" type="box" pos="0 0 0" size="1.5 14 1.5" rgba="0.59 0.8 1 .8" mass=".05" contype="1" conaffinity="1" density="1250"/>
                <site name="bodyLeft" pos="0 -14 1.5" size="3.75" rgba="0.031 0.509 0.96 .8"/>
                <site name="bodyRight" pos="0 14 1.5" size="3.75" rgba="0.031 0.509 0.96 .8"/>
            </body>

            <!--6 different bodies resemble the 6 connection points of the fish tail-->
            <body pos="100 3.25 0" name="firstPeg">
