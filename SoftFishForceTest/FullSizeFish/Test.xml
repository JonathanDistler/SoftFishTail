                <site name="tail0left" pos="12 -23 75" size="3.75" rgba=".9 .6 .5 .5"/>
                <site name="tail0right" pos="12 23 75" size="3.75" rgba=".9 .6 .5 .5"/>

               
                <body pos="25 0 0" name="secondPeg">
                    <geom name="joint1" type="cylinder" pos="0 0 75" size="5 55" rgba=".9 .2 .2 1" mass="0" contype="0" conaffinity="0"/>
                    <joint name="joint1"/>
                    <geom name="joint1-tail1" type="box" pos="12 0 75" size="2 20 2" rgba=".9 .9 .9 1" mass="0.001" fluidshape="ellipsoid" contype="0" conaffinity="0"/>
