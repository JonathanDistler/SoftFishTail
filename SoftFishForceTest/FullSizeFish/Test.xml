                    <geom name="tail1left" type="box" pos="12 10 75" size="2 10 2" rgba=".5 .7 .9 1" mass="0.01" fluidshape="ellipsoid" contype="0" conaffinity="0"/>
                    <geom name="tail1right" type="box" pos="12 -10 75" size="2 10 2" rgba=".5 .7 .9 1" mass="0.01" fluidshape="ellipsoid" contype="0" conaffinity="0"/>
                    <site name="tail1left" pos="12 -18 75" size="3.75" rgba=".9 .6 .5 .5"/>
                    <site name="tail1right" pos="12 18 75" size="3.75" rgba=".9 .6 .5 .5"/>

                    
                    <body pos="25 0 0" name="thirdPeg">
                        <geom name="joint2" type="cylinder" pos=" 0 0 75" size="5 50" rgba=".9 .2 .2 1" mass="0" contype="0" conaffinity="0"/>
                        <joint name="joint2"/>
                        <geom name="joint2-tail2" type="box" pos="12 0 75" size="2 15 2" rgba=".9 .9 .9 1" mass="0.001" fluidshape="ellipsoid" contype="0" conaffinity="0"/>
                        <geom name="tail2" type="box" pos="12 0 75" size="12.5 2 46.5" rgba=".5 .7 .9 1" mass="0.01" fluidshape="ellipsoid" contype="0" conaffinity="0"/>
                        <site name="marker6" pos="12 0 75" size="3.75" material="marker"/>

                        <geom name="tail2left" type="box" pos="12 7.5 75" size="2 7.5 2" rgba=".5 .7 .9 1" mass="0.01" fluidshape="ellipsoid" contype="0" conaffinity="0"/>
                        <geom name="tail2right" type="box" pos="12 -7.5 75" size="2 7.5 2" rgba=".5 .7 .9 1" mass="0.01" fluidshape="ellipsoid" contype="0" conaffinity="0"/>
                        <site name="tail2left" pos="12 -12.5 75" size="3.75" rgba=".9 .6 .5 .5"/>
                        <site name="tail2right" pos="12 12.5 75" size="3.75" rgba=".9 .6 .5 .5"/>

                        
                        <body pos="25 0 0" name="fourthPeg">
                            <geom name="joint3" type="cylinder" pos=" 0 0 75" size="5 43" rgba=".9 .2 .2 1" mass="0" contype="0" conaffinity="0"/>
                            <joint name="joint3"/>
                            <geom name="joint3-tail3" type="box" pos="12 0 75" size="2 10 2" rgba=".9 .9 .9 1" mass="0.001" fluidshape="ellipsoid" contype="0" conaffinity="0"/>
                            <geom name="tail3" type="box" pos="12 0 75" size="12.5 2 38.5" rgba=".5 .7 .9 1" mass="0.01" fluidshape="ellipsoid" contype="0" conaffinity="0"/>
                            <site name="marker7" pos="12 0 75" size="5" material="marker"/>

                            <geom name="tail3left" type="box" pos="12 5 75" size="2 5 2" rgba=".5 .7 .9 1" mass="0.01" fluidshape="ellipsoid" contype="0" conaffinity="0"/>
                            <geom name="tail3right" type="box" pos="12 -5 75" size="2 5 2" rgba=".5 .7 .9 1" mass="0.01" fluidshape="ellipsoid" contype="0" conaffinity="0"/>
                            <site name="tail3left" pos="12 -7.5 75" size="3.75" rgba=".9 .6 .5 .5"/>
                            <site name="tail3right" pos="12 7.5 75" size="3.75" rgba=".9 .6 .5 .5"/>
                            
                            
                            <body pos="25 0 0" name="fifthPeg">
                                <geom name="joint4" type="cylinder" pos=" 0 0 75" size="5 34" rgba=".9 .2 .2 1" mass="0" contype="0" conaffinity="0"/>
                                <joint name="joint4"/>
                                <geom name="joint4-tail4" type="box" pos="12 0 75" size="2 7.5 2" rgba=".9 .9 .9 1" mass="0.001" fluidshape="ellipsoid" contype="0" conaffinity="0"/>
                                <geom name="tail4" type="box" pos="12 0 75" size="12.5 2 29.5" rgba=".5 .7 .9 1" mass="0.01" fluidshape="ellipsoid" contype="0" conaffinity="0"/>
                                <site name="marker8" pos="12 0 75" size="3.75" material="marker"/>

                                <geom name="tail4left" type="box" pos="12 3.75 75" size="2 3.75 2" rgba=".5 .7 .9 1" mass="0.01" fluidshape="ellipsoid" contype="0" conaffinity="0"/>
                                <geom name="tail4right" type="box" pos="12 -3.75 75" size="2 3.75 2" rgba=".5 .7 .9 1" mass="0.01" fluidshape="ellipsoid" contype="0" conaffinity="0"/>
                                <site name="tail4left" pos="12 -6.5 75" size="3.75" rgba=".9 .6 .5 .5"/>
                                <site name="tail4right" pos="12 6.5 75" size="3.75" rgba=".9 .6 .5 .5"/>
                                
                                <!--Apparently there is no tail4-joint5 geometry-->
                                <body pos="25 0 0" name="sixthPeg">
                                    <geom name="joint5" type="cylinder" pos=" 0 0 75" size="5 25" rgba=".9 .2 .2 1" mass="0" contype="0" conaffinity="0"/>
                                    <joint name="joint5"/>
