                <site name="marker4" pos="12 0 75" size="3.75" material="marker"/>

                <geom name="tail0left" type="box" pos="12 12.5 75" size="2 12.5 2" rgba=".5 .7 .9 1" mass=".01" fluidshape="ellipsoid" contype="0" conaffinity="0"/>
