            <geom type="box" size=".05 .05 .05" rgba="0 0 1 1" contype="1" mass=".1" conaffinity="1"/>
            <site name="ft_sensor" pos="0 0 0" rgba="1 1 1 1" size=".05 .05 .05" type="box" /> 
        </body>

        <body name="FishTank" pos="0 -.42 -.7">
