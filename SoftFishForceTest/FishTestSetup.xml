    <!--tail organization-->
    <tendon>
        <spatial name="tendonLeft">
            <site site="bodyLeft"/>
            <site site="tail0left"/>
            <site site="tail1left"/>
            <site site="tail2left"/>
            <site site="tail3right"/>
            <site site="tail4right"/>
            <site site="tail5right"/>
        </spatial>
        <spatial name="tendonRight" >
            <site site="bodyRight"/>
            <site site="tail0right"/>
