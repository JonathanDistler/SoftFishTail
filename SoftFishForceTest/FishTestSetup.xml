            <site site="tail2right"/>
            <site site="tail3left"/>
            <site site="tail4left"/> 
            <site site="tail5left"/>           
        </spatial>
    </tendon>

    <!--creates actuator to control motor and to couple the forceamplifier. Measures the force with a sensor-->
